{
  "kind": "collectionType",
  "collectionName": "services",
  "info": {
    "singularName": "service",
    "pluralName": "services",
    "displayName": "Service",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true,
      "maxLength": 80,
      "minLength": 10
    },
    "category": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::category.category",
      "inversedBy": "services"
    },
    "skills": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::skill.skill",
      "inversedBy": "services"
    },
    "media": {
      "type": "media",
      "multiple": true,
      "required": true,
      "allowedTypes": [
        "images",
        "videos",
        "audios"
      ]
    },
    "packages": {
      "type": "dynamiczone",
      "components": [
        "pricing.basic-package",
        "pricing.standard-package",
        "pricing.premium-package"
      ],
      "required": true,
      "min": 1,
      "max": 3
    },
    "addons": {
      "type": "component",
      "repeatable": true,
      "component": "pricing.addon",
      "max": 3
    },
    "faq": {
      "type": "component",
      "repeatable": true,
      "component": "pricing.faq",
      "max": 5
    },
    "freelancer": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::freelancer.freelancer",
      "inversedBy": "services"
    },
    "status": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::status.status",
      "inversedBy": "services"
    },
    "price": {
      "type": "integer",
      "max": 50000,
      "min": 10
    },
    "time": {
      "type": "integer",
      "min": 1
    },
    "description": {
      "type": "text",
      "minLength": 80,
      "maxLength": 5000,
      "required": true
    },
    "location": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::location.location",
      "inversedBy": "services"
    }
  }
}
